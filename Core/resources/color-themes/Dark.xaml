<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Color_StrokeSelected">Red</Color>
    <Color x:Key="Color_StrokeDefault">LightYellow</Color>
    <SolidColorBrush x:Key="Brush_StrokeSelected" Color="Red"/>
	<SolidColorBrush x:Key="Brush_StrokeDefault" Color="LightYellow"/>

	<SolidColorBrush x:Key="Brush_Foreground" Color="WhiteSmoke"/>
	<SolidColorBrush x:Key="Brush_Background" Color="Black"/>

	<SolidColorBrush x:Key="Brush_TextDisabled" Color="Gray"/>
	
	<SolidColorBrush x:Key="Brush_LogMessageInfo" Color="White"/>
	<SolidColorBrush x:Key="Brush_LogMessageDebug" Color="Gray"/>
	<SolidColorBrush x:Key="Brush_LogMessageWarn" Color="Yellow"/>
	<SolidColorBrush x:Key="Brush_LogMessageError" Color="IndianRed"/>
	
	<SolidColorBrush x:Key="Brush_GridSplitterBackground" Color="SlateGray"/>
	<SolidColorBrush x:Key="Brush_GridSplitterHovered" Color="LightSlateGray"/>

	<SolidColorBrush x:Key="Brush_MenuBarForeground" Color="White"/>
    <SolidColorBrush x:Key="Brush_MenuBarBackground" Color="DarkSlateGray"/>
	<SolidColorBrush x:Key="Brush_MenuBarBorder" Color="SlateGray"/>
	
	<SolidColorBrush x:Key="Brush_MenuItemForeground" Color="Black"/>

</ResourceDictionary>